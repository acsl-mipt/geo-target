"Модуль тестовых типов и компонентов (проверка синтаксиса, резолвинга и поддержки сложных типов)"
namespace test

alias f32 float:32
alias u8 uint:8

type f32_arr [f32, 4]
type u8_dyn_arr [u8, 3..8]
type u8_dyn_arr2 [u8, 10..*]

type u8_enum enum u8 (ZERO = 0, ONE = 1, TEN = 10)

type sub_struct
struct
(
    u8 first,
    u8_enum second,
    f32_arr arr
)

type struct_test
struct
(
    u8 u8,
    f32 f32,
    u8_dyn_arr dyn_arr,
    f32_arr arr,
    int:64 int_64,
    [sub_struct, 2..3] sub_structs
)

component SubComp
{
    parameters
    (
        u8 a,
        uint:16 b,
        [sub_struct, 2] sub_structs
    )
}

component TestComp with SubComp
{
    parameters
    (
        u8 param1,
    )

    command cmdZero id:0()
    command cmdOne id:1(u8 arg0, f32_arr arg1, u8_dyn_arr arg2, struct_test arg3)

    status msgZero id:0
    (
        param1,
    )
    event msgOne id:1 u8_enum
    (
        param1,
        SubComp.sub_structs[1].arr[1]
    )
}